{
  "name": "@angel_pro/components-vue3",
  "private": false,
  "description": "智运vue3组件抽离",
  "version": "0.0.13",
  "type": "module",
  "scripts": {
    "start:dev": "vite --config ./build/base.config.ts",
    "build-lib": "vite build --config ./build/lib.config.ts",
    "build-tsc": "vue-tsc --noEmit && vite build --config ./build/base.config.ts",
    "patch": "npm version patch -m 'build-lib: release 0.0.1'",
    "preview": "vite preview",
    "publish-lib": "npm run build-lib && npm publish",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "main": "./dist/components.umd.js",
  "module": "./dist/components.es.js",
  "types": "./dist/components.d.ts",
  "license": "MIT",
  "authors": {
    "name": "angel_pro",
    "email": "angeliporo@gmail.com"
  },
  "files": [
    "dist/*"
  ],
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "axios": "^1.4.0",
    "element-plus": "2.3.1",
    "lodash": "^4.17.21",
    "print-js": "^1.6.0",
    "sortablejs": "^1.15.2",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/sortablejs": "^1.15.7",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/coverage-v8": "^0.32.2",
    "@vue/test-utils": "^2.4.0-alpha.2",
    "happy-dom": "^9.20.3",
    "less": "^4.2.0",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^3.7.1",
    "vite-plugin-md": "^0.21.5",
    "vitest": "^0.32.2",
    "vue-tsc": "^1.4.2"
  }
}
